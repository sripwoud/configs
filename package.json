  },
  "packageManager": "pnpm@9.0.5",
  "devDependencies": {
    "@changesets/cli": "^2.27.2",
    "@sripwoud/dprint-config": "^1.0.3",
    "dprint": "^0.45.1",
    "js-yaml": "^4.1.0"
  }
}
